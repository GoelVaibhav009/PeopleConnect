            /*btn*/
            .btn {
            border: none;
            padding: 15px 30px;
            line-height: initial;
            border-radius: 30px;
            -webkit-transition: 0.5s;
            transition: 0.5s;
            position: relative;
            z-index: 1;
            font-family: "Montserrat", sans-serif;
            font-size: 15px;
            font-weight: 700;
            }

            .btn.disabled, .btn:disabled {
            opacity: 1;
            }

            .btn-primary {
            color: #0e101b;
            background-color: #ffcf58;
            }

            .btn-primary::before {
            content: '';
            position: absolute;
            left: 0;
            top: 0;
            width: 0;
            height: 100%;
            border-radius: 30px 0 0 30px;
            z-index: -1;
            background: #2d9bea;
            opacity: 0;
            visibility: hidden;
            -webkit-transition: 0.5s;
            transition: 0.5s;
            }

            .btn-primary::after {
            content: '';
            position: absolute;
            right: 0;
            top: 0;
            width: 0;
            height: 100%;
            border-radius: 0 30px 30px 0;
            z-index: -1;
            background: #2d9bea;
            opacity: 0;
            visibility: hidden;
            -webkit-transition: 0.5s;
            transition: 0.5s;
            }

            .btn-primary.disabled, .btn-primary:disabled {
            color: #0e101b;
            background-color: #ffcf58;
            }

            .btn-primary:hover, .btn-primary:focus {
            color: #ffffff !important;
            background-color: #ffcf58 !important;
            -webkit-box-shadow: unset !important;
            box-shadow: unset !important;
            }

            .btn-primary:hover::before, .btn-primary:focus::before {
            width: 60%;
            opacity: 1;
            visibility: visible;
            }

            .btn-primary:hover::after, .btn-primary:focus::after {
            width: 60%;
            opacity: 1;
            visibility: visible;
            }

